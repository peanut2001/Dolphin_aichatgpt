# 生产环境配置

# 应用标题
VITE_APP_TITLE=Dolphin AI

# API基础地址
VITE_API_BASE_URL=https://api.dolphin-ai.com/api

# WebSocket地址
VITE_WS_URL=wss://api.dolphin-ai.com/ws

# 是否启用Mock数据
VITE_USE_MOCK=false

# 是否启用调试模式
VITE_DEBUG=false

# 日志级别 (error | warn | info | debug)
VITE_LOG_LEVEL=error

# 是否启用性能监控
VITE_ENABLE_PERFORMANCE=true

# 文件上传地址
VITE_UPLOAD_URL=https://api.dolphin-ai.com/api/file/upload

# CDN地址
VITE_CDN_URL=https://cdn.dolphin-ai.com

# 第三方服务配置
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# 错误监控服务
VITE_SENTRY_DSN=https://xxxxxxxxxx@sentry.io/xxxxxxx

# 地图服务API Key
VITE_MAP_API_KEY=your_map_api_key_here
